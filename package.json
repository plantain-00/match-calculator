{
  "name": "match-calculator",
  "version": "1.0.0",
  "description": "A match calculator",
  "main": "index.js",
  "scripts": {
    "build": "rimraf index.bundle-*.js index.bundle-*.css && npm run file2variable && tsc && npm run clean-css && webpack && rev-static && npm run sw-precache",
    "lint": "tslint \"*.ts\" && npm run standard",
    "file2variable": "file2variable-cli *.template.html -o variables.ts --html-minify",
    "sw-precache": "sw-precache --config sw-precache.config.js && npm run uglifyjs",
    "uglifyjs": "uglifyjs service-worker.js -o service-worker.bundle.js",
    "clean-css": "cleancss index.css ./node_modules/github-fork-ribbon-css/gh-fork-ribbon.css ./node_modules/tab-container-component/dist/tab-container.min.css -o index.bundle.css",
    "standard": "standard \"**/*.config.js\"",
    "fix": "standard --fix \"**/*.config.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/match-calculator.git"
  },
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/match-calculator/issues"
  },
  "homepage": "https://github.com/plantain-00/match-calculator#readme",
  "devDependencies": {
    "@types/json5": "0.0.29",
    "ajv": "5.2.0",
    "clean-css-cli": "4.1.6",
    "file2variable-cli": "1.3.0",
    "github-fork-ribbon-css": "0.2.1",
    "json5": "0.5.1",
    "rev-static": "2.5.0",
    "rimraf": "2.6.1",
    "standard": "10.0.2",
    "sw-precache": "5.2.0",
    "tab-container-component": "1.1.0",
    "tslib": "1.7.1",
    "tslint": "5.4.3",
    "typescript": "2.4.1",
    "uglify-js": "3.0.22",
    "vue": "2.3.4",
    "vue-class-component": "5.0.2",
    "webpack": "3.0.0"
  }
}
