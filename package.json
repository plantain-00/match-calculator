{
  "name": "match-calculator",
  "version": "1.0.0",
  "description": "A match calculator",
  "main": "index.js",
  "scripts": {
    "build": "rimraf index.bundle-*.js && tsc && npm run clean-css && webpack && rev-static && npm run sw-precache",
    "lint": "tslint \"*.ts\"",
    "file2variable": "file2variable-cli index.template.html -o variables.ts --html-minify",
    "sw-precache": "sw-precache --config sw-precache.config.js && npm run uglifyjs",
    "uglifyjs": "uglifyjs service-worker.js -o service-worker.bundle.js",
    "clean-css": "cleancss index.css -o index.bundle.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/match-calculator.git"
  },
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/match-calculator/issues"
  },
  "homepage": "https://github.com/plantain-00/match-calculator#readme",
  "devDependencies": {
    "@types/json5": "0.0.29",
    "ajv": "5.0.1",
    "clean-css-cli": "4.0.12",
    "file2variable-cli": "1.3.0",
    "json5": "0.5.1",
    "rev-static": "2.1.2",
    "rimraf": "2.6.1",
    "sw-precache": "5.1.1",
    "tslib": "1.6.1",
    "tslint": "5.1.0",
    "typescript": "2.3.2",
    "uglify-js": "3.0.11",
    "vue": "2.3.0",
    "vue-class-component": "5.0.1",
    "webpack": "2.4.1"
  }
}
